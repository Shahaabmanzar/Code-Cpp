        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".basics_activity"></activity>
        <activity android:name=".coretopics_activity" />
        <activity android:name=".Editor" />
        <activity android:name=".envr_setup" />
        <activity android:name=".whycpp" />
        <activity android:name=".gt_started_activity" />
        <activity android:name=".main_menu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
